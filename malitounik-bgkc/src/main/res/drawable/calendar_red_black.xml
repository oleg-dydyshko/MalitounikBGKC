<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary_black2" />
            <stroke android:width="1sp" android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary_black" />
            <stroke android:width="1sp" android:color="@android:color/transparent" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary_black" />
            <stroke android:width="1sp" android:color="@android:color/transparent" />
        </shape>
    </item>
</selector>
